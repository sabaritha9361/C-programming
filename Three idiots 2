#include <stdio.h>

int main() {
    int x1, y1, x2, y2;
    float mid_x, mid_y;

    // Reading input
    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);

    // Calculate midpoint
    mid_x = (x1 + x2) / 2.0;
    mid_y = (y1 + y2) / 2.0;

    // Output result
    printf("Binoy's house is located at (%.1f,%.1f)\n", mid_x, mid_y);

    return 0;
}
