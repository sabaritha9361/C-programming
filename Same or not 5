#include <stdio.h>

int main() {

    int n;

    scanf("%d", &n);  // Array size

    int arr[n];

    int even = 0, odd = 0;

    // Array input

    for (int i = 0; i < n; i++) {

        scanf("%d", &arr[i]);

        // Count even and odd

        if (arr[i] % 2 == 0)

            even++;

        else

            odd++;

    }

    // Decision based on even and odd counts

    if (even == n)

        printf("Even\n");

    else if (odd == n)

        printf("Odd\n");

    else

        printf("Mixed\n");

    return 0;

}
